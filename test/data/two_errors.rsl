scheme two_errors =
    class
      type
          Database = Person-set,  Person = Text
      value
        register : Person >< Database -> Database
        register(p,db) is db1 union {p}
      test_case
        [t1] register("Henrik", register("Anne", {})) 
      value
        factorial : Nat -> Nat
        factorial(n) is if n2 <= 1 then 1 else n * factorial(n - 1) end
      test_case
        [t2] factorial(5)
    end
  